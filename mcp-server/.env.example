# n8n API Configuration (for workflow management)
N8N_API_URL=https://your-n8n-instance.com/api/v1
N8N_BASE_URL=https://your-n8n-instance.com
N8N_API_KEY=your_api_key_here

# n8n Version (for rebuilding database with correct node versions)
# Format: n8n@X.Y.Z (e.g., n8n@2.0.3, n8n@1.100.1)
N8N_VERSION=n8n@2.0.3

# Database Paths (mcp-server now uses its own independent database)
N8N_MCP_DB_PATH=./data/nodes.db
N8N_MCP_TEMPLATES_DB_PATH=./data/templates.db

# Logging
LOG_LEVEL=info
MCP_MODE=stdio
